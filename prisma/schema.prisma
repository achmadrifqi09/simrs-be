generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["relationJoins"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model AppClient {
  client_id     String    @id @default(uuid())
  client_secret String    @default(uuid())
  nama_client   String    @db.VarChar(100)
  created_at    DateTime  @default(now())
  created_by    Int       @default(0)
  modified_at   DateTime? @db.DateTime(0)
  modified_by   Int?      @default(0)
  deleted_at    DateTime? @db.DateTime(0)
  deleted_by    Int?      @default(0)
  restored_at   DateTime? @db.DateTime(0)
  restored_by   Int?      @default(0)
  is_deleted    Boolean   @default(false)
  is_restored   Boolean   @default(false)

  @@index([nama_client])
  @@index([is_deleted, created_at])
  @@map("db_client")
}

model Registration {
  id                 Int               @id @default(autoincrement())
  nomor_registrasi   String            @db.VarChar(50)
  id_antrian         Int
  antrian            Queue             @relation(fields: [id_antrian], references: [id_antrian])
  id_online          Int?
  kode_rm            String?           @db.VarChar(50)
  tgl_daftar         DateTime
  id_asuransi        Int?
  nomor_asuransi     String?           @db.VarChar(50)
  asuransi           Insurance?        @relation(fields: [id_asuransi], references: [id])
  no_rujukan         String?           @db.VarChar(50)
  status_rujukan     Int?
  nomor_rujuk_balik  String?           @db.VarChar(50)
  no_sep             String?           @db.VarChar(50)
  cob                Int?
  no_cob             String?           @db.VarChar(50)
  status_bpjs        Int               @default(0)
  status_keuangan    Int               @default(0)
  status_inap        Int               @default(0)
  status_koding      Int               @default(0)
  status_bayar       Int               @default(0)
  tgl_pulang         DateTime?
  nama_wali          String?           @db.VarChar(50)
  telp_wali          String?           @db.VarChar(50)
  id_hub_wali        Int?
  hub_wali           FamilyStatus?     @relation(fields: [id_hub_wali], references: [id])
  asal_rujukan       String?           @db.VarChar(100)
  nama_perujuk       String?           @db.VarChar(100)
  tgl_rujukan        DateTime?         @db.Date
  ket_rujukan        String?           @db.Text
  kode_antrian_poli  String?           @db.VarChar(10)
  nomor_antrian_poli String?           @db.VarChar(3)
  status_batal       Int               @default(0)
  keterangan_batal   String?           @db.Text
  task_id            Int?
  created_at         DateTime          @default(now())
  created_by         Int               @default(0)
  modified_at        DateTime?         @db.DateTime(0)
  modified_by        Int?              @default(0)
  deleted_at         DateTime?         @db.DateTime(0)
  deleted_by         Int?              @default(0)
  restored_at        DateTime?         @db.DateTime(0)
  restored_by        Int?              @default(0)
  is_deleted         Boolean           @default(false)
  is_restored        Boolean           @default(false)
  biaya_pendaftaran  RegistrationFee[]

  @@index([nomor_registrasi])
  @@index([kode_rm])
  @@index([id_antrian])
  @@index([id_asuransi])
  @@index([id_hub_wali])
  @@index([no_sep])
  @@index([tgl_daftar])
  @@index([status_bpjs, status_keuangan, status_inap, status_koding, status_bayar])
  @@index([is_deleted])
  @@map("db_pendaftaran")
}

model OnlineRegistration {
  id_antrian              Int            @id @default(autoincrement())
  antrian                 Queue          @relation(fields: [id_antrian], references: [id_antrian])
  jenis_pasien            Int // 1:lama, 2:baru
  jenis_penjamin          Int // 1:umum, 2:bpjs
  asal_pengambilan        Int // 1:online, 2:mjkn
  kode_rm                 String?        @db.VarChar(20)
  nama_pasien             String         @db.VarChar(100)
  identitas_pasien        Int // 1:KTP, 2:paspor, dsb
  no_identitas            String         @db.VarChar(100)
  no_bpjs                 String?        @db.VarChar(50)
  jenis_kelamin           Int // 1:laki, 2:perempuan
  tempat_lahir            String         @db.VarChar(100)
  tgl_lahir               DateTime
  hp                      String         @db.VarChar(20)
  email                   String         @db.VarChar(100)
  id_ms_status_kawin      Int            @default(0)
  status_kawin            MaritalStatus  @relation(fields: [id_ms_status_kawin], references: [id_ms_status_kawin])
  id_ms_agama             Int
  agama                   Religion       @relation(fields: [id_ms_agama], references: [id_ms_agama])
  id_ms_golongan_darah    Int            @default(0)
  golongan_darah          BloodType      @relation(fields: [id_ms_golongan_darah], references: [id_ms_golongan_darah])
  id_ms_pendidikan        Int
  pendidikan              EducationLevel @relation(fields: [id_ms_pendidikan], references: [id_ms_tingkat_pendidikan])
  nama_pekerjaan          String?        @db.VarChar(100)
  suku                    String?        @db.VarChar(50)
  id_ms_negara_asal       Int
  negara_asal             Country        @relation("pendaftaran_negara_asal", fields: [id_ms_negara_asal], references: [id])
  id_ms_propinsi_asal     String
  provinsi_asal           Province       @relation("pendaftaran_provinsi_asal", fields: [id_ms_propinsi_asal], references: [id])
  id_ms_kota_asal         String
  kota_asal               Regency        @relation("pendaftaran_kota_asal", fields: [id_ms_kota_asal], references: [id])
  id_ms_kecamatan_asal    String
  kecamatan_asal          District       @relation("pendaftaran_kecamatan_asal", fields: [id_ms_kecamatan_asal], references: [id])
  id_ms_desa_asal         String
  desa_asal               Village        @relation("pendaftaran_desa_asal", fields: [id_ms_desa_asal], references: [id])
  rw_asal                 String         @db.VarChar(5)
  rt_asal                 String         @db.VarChar(5)
  alamat_asal             String         @db.Text
  kode_pos_asal           String         @db.VarChar(10)
  alamatgab_asal          String         @db.Text
  id_ms_negara_tinggal    Int
  negara_tinggal          Country        @relation("pendaftaran_negara_tinggal", fields: [id_ms_negara_tinggal], references: [id])
  id_ms_provinsi_tinggal  String
  provinsi_tinggal        Province       @relation("pendaftaran_provinsi_tinggal", fields: [id_ms_provinsi_tinggal], references: [id])
  id_ms_kota_tinggal      String
  kota_tinggal            Regency        @relation("pendaftaran_kota_tinggal", fields: [id_ms_kota_tinggal], references: [id])
  id_ms_desa_tinggal      String
  desa_tinggal            Village        @relation("pendaftaran_desa_tinggal", fields: [id_ms_desa_tinggal], references: [id])
  id_ms_kecamatan_tinggal String
  kecamatan_tinggal       District       @relation("pendaftaran_kecamatan_tinggal", fields: [id_ms_kecamatan_tinggal], references: [id])
  rw_tinggal              String         @db.VarChar(5)
  rt_tinggal              String         @db.VarChar(5)
  alamat_tinggal          String         @db.Text
  kode_pos_tinggal        String         @db.VarChar(10)
  alamatgab_tinggal       String         @db.Text
  tgl_kunjungan           DateTime
  no_rujukan              String         @db.VarChar(50)
  kode_booking            String         @db.VarChar(50)
  id_jadwal_dokter        Int
  status                  Int            @default(1) // 1:aktif, 2:batal
  keterangan_batal        String         @db.Text
  tgl_cekin               DateTime
  id_ms_loket_antrian     Int
  loket                   Counter        @relation(fields: [id_ms_loket_antrian], references: [id_ms_loket_antrian])
  created_at              DateTime       @default(now())
  created_by              Int            @default(0)
  modified_at             DateTime?      @db.DateTime(0)
  modified_by             Int?           @default(0)
  deleted_at              DateTime?      @db.DateTime(0)
  deleted_by              Int?           @default(0)
  restored_at             DateTime?      @db.DateTime(0)
  restored_by             Int?           @default(0)
  is_deleted              Boolean        @default(false)
  is_restored             Boolean        @default(false)

  @@index([kode_rm])
  @@index([no_identitas])
  @@index([no_bpjs])
  @@index([kode_booking])
  @@index([tgl_kunjungan])
  @@index([id_jadwal_dokter])
  @@index([status])
  @@index([id_ms_status_kawin])
  @@index([id_ms_agama])
  @@index([id_ms_golongan_darah])
  @@index([id_ms_pendidikan])
  @@index([id_ms_negara_asal])
  @@index([id_ms_propinsi_asal])
  @@index([id_ms_kota_asal])
  @@index([id_ms_kecamatan_asal])
  @@index([id_ms_desa_asal])
  @@index([id_ms_negara_tinggal])
  @@index([id_ms_provinsi_tinggal])
  @@index([id_ms_kota_tinggal])
  @@index([id_ms_kecamatan_tinggal])
  @@index([id_ms_desa_tinggal])
  @@index([id_ms_loket_antrian])
  @@index([is_deleted])
  @@map("db_pendaftaran_online")
}

model RegistrationFee {
  id                         Int          @id @default(autoincrement())
  id_pendaftaran             Int
  pendaftaran                Registration @relation(fields: [id_pendaftaran], references: [id])
  biaya_daftar               String       @db.VarChar(20)
  diskon_daftar              String       @db.VarChar(20)
  biaya_kartu                String       @default("0000-00-00 00:00:00") @db.VarChar(20)
  diskon_kartu               String       @db.VarChar(20)
  biaya_dokter               String       @db.VarChar(20)
  diskon_dokter              String       @db.VarChar(20)
  tgl_billing_daftar         DateTime
  tgl_billing_daftar_selesai DateTime
  created_at                 DateTime     @default(now())
  created_by                 Int          @default(0)
  modified_at                DateTime?    @db.DateTime(0)
  modified_by                Int?         @default(0)
  deleted_at                 DateTime?    @db.DateTime(0)
  deleted_by                 Int?         @default(0)
  restored_at                DateTime?    @db.DateTime(0)
  restored_by                Int?         @default(0)
  is_deleted                 Boolean      @default(false)
  is_restored                Boolean      @default(false)

  @@index([id_pendaftaran])
  @@index([tgl_billing_daftar])
  @@index([is_deleted])
  @@map("db_biaya_pendaftaran")
}

model Insurance {
  id            Int            @id @default(autoincrement())
  nama_asuransi String         @db.VarChar(100)
  status        Int            @default(1)
  created_at    DateTime       @default(now())
  created_by    Int            @default(0)
  modified_at   DateTime?      @db.DateTime(0)
  modified_by   Int?           @default(0)
  deleted_at    DateTime?      @db.DateTime(0)
  deleted_by    Int?           @default(0)
  restored_at   DateTime?      @db.DateTime(0)
  restored_by   Int?           @default(0)
  is_deleted    Boolean        @default(false)
  is_restored   Boolean        @default(false)
  pendaftaran   Registration[]

  @@index([nama_asuransi])
  @@index([status, is_deleted])
  @@map("db_asuransi")
}

model User {
  id_user       Int          @id @default(autoincrement())
  id_pegawai    Int
  nama_user     String       @db.VarChar(100)
  email_user    String       @unique @db.VarChar(100)
  hp_user       String       @db.VarChar(20)
  password_user String       @db.Text
  status        Boolean      @default(true)
  created_at    DateTime     @default(now())
  created_by    Int          @default(0)
  modified_at   DateTime?    @db.DateTime(0)
  modified_by   Int?         @default(0)
  deleted_at    DateTime?    @db.DateTime(0)
  deleted_by    Int?         @default(0)
  restored_at   DateTime?    @db.DateTime(0)
  restored_by   Int?         @default(0)
  is_deleted    Boolean      @default(false)
  is_restored   Boolean      @default(false)
  akses_user    UserAccess[]

  @@index([email_user])
  @@index([id_pegawai])
  @@index([nama_user])
  @@index([hp_user])
  @@index([status, is_deleted])
  @@map("db_user")
}

model Menu {
  id               Int                     @id @default(autoincrement())
  parent_id        Int?
  order            Int
  label            String
  icon             String?
  pathname         String?
  tag              String                  @unique
  is_submenu       Boolean
  status           Boolean                 @default(true)
  parent           Menu?                   @relation("MenuToMenu", fields: [parent_id], references: [id])
  children         Menu[]                  @relation("MenuToMenu")
  created_at       DateTime                @default(now())
  created_by       Int                     @default(0)
  modified_at      DateTime?               @db.DateTime(0)
  modified_by      Int?                    @default(0)
  deleted_at       DateTime?               @db.DateTime(0)
  deleted_by       Int?                    @default(0)
  restored_at      DateTime?               @db.DateTime(0)
  restored_by      Int?                    @default(0)
  is_deleted       Boolean                 @default(false)
  is_restored      Boolean                 @default(false)
  izin_akses_level AccessLevelPermission[]

  @@index([parent_id])
  @@index([pathname])
  @@index([tag])
  @@index([is_deleted])
  @@index([is_submenu, order])
  @@index([status, is_deleted, order])
  @@map("db_menu")
}

model AccessLevel {
  id          Int       @id @default(autoincrement())
  nama        String
  status      Boolean   @default(true)
  created_at  DateTime  @default(now())
  created_by  Int       @default(0)
  modified_at DateTime? @db.DateTime(0)
  modified_by Int?      @default(0)
  deleted_at  DateTime? @db.DateTime(0)
  deleted_by  Int?      @default(0)
  restored_at DateTime? @db.DateTime(0)
  restored_by Int?      @default(0)
  is_deleted  Boolean   @default(false)
  is_restored Boolean   @default(false)

  izin_level_akses AccessLevelPermission[]
  akses_user       UserAccess[]

  @@index([nama])
  @@index([status])
  @@index([is_deleted])
  @@map("db_level_akses")
}

model AccessLevelPermission {
  id             Int       @id @default(autoincrement())
  id_level_akses Int
  id_menu        Int
  can_view       Boolean
  can_create     Boolean
  can_update     Boolean
  can_delete     Boolean
  created_at     DateTime  @default(now())
  created_by     Int       @default(0)
  modified_at    DateTime? @db.DateTime(0)
  modified_by    Int?      @default(0)
  deleted_at     DateTime? @db.DateTime(0)
  deleted_by     Int?      @default(0)
  restored_at    DateTime? @db.DateTime(0)
  restored_by    Int?      @default(0)
  is_deleted     Boolean   @default(false)
  is_restored    Boolean   @default(false)

  menu        Menu        @relation(fields: [id_menu], references: [id])
  akses_level AccessLevel @relation(fields: [id_level_akses], references: [id])

  @@index([id_level_akses, id_menu])
  @@index([is_deleted])
  @@map("db_izin_level_akses")
}

model UserAccess {
  id             Int       @id @default(autoincrement())
  id_user        Int
  id_level_akses Int
  id_pegawai     Int
  created_at     DateTime  @default(now())
  created_by     Int       @default(0)
  modified_at    DateTime? @db.DateTime(0)
  modified_by    Int?      @default(0)
  deleted_at     DateTime? @db.DateTime(0)
  deleted_by     Int?      @default(0)
  restored_at    DateTime? @db.DateTime(0)
  restored_by    Int?      @default(0)
  is_deleted     Boolean   @default(false)
  is_restored    Boolean   @default(false)

  user        User        @relation(fields: [id_user], references: [id_user])
  level_akses AccessLevel @relation(fields: [id_level_akses], references: [id])

  @@index([id_user])
  @@index([id_level_akses])
  @@index([id_pegawai])
  @@index([is_deleted])
  @@map("db_akses_user")
}

model Patient {
  id_pasien               Int            @id @default(autoincrement())
  kode_rm                 String         @default("") @db.VarChar(20)
  nama_pasien             String         @db.VarChar(100)
  tempat_lahir            String         @db.VarChar(100)
  tgl_lahir               DateTime       @db.Date
  jenis_kelamin           Int
  id_warga_negara         Int
  identitas_pasien        Int
  no_identitas            String         @db.VarChar(100)
  no_bpjs                 String         @db.VarChar(50)
  no_hp                   String         @db.VarChar(20)
  id_ms_negara_tinggal    Int
  negara_tinggal          Country        @relation("negara_tinggal_pasien", fields: [id_ms_negara_tinggal], references: [id])
  id_ms_provinsi_tinggal  String
  provinsi_tinggal        Province       @relation("provinsi_tinggal_pasien", fields: [id_ms_provinsi_tinggal], references: [id])
  id_ms_kota_tinggal      String
  kota_tinggal            Regency        @relation("kota_tinggal_pasien", fields: [id_ms_kota_tinggal], references: [id])
  id_ms_kecamatan_tinggal String
  kecamatan_tinggal       District       @relation("kecamatan_tinggal_pasien", fields: [id_ms_kecamatan_tinggal], references: [id])
  id_ms_desa_tinggal      String
  desa_tinggal            Village        @relation("desa_tinggal_pasien", fields: [id_ms_desa_tinggal], references: [id])
  alamat_tinggal          String         @db.Text
  rt_tinggal              String         @db.VarChar(5)
  rw_tinggal              String         @db.VarChar(5)
  kode_pos_tinggal        String?        @db.VarChar(10)
  alamatgab_tinggal       String         @db.Text
  suku                    String?        @db.VarChar(50)
  nama_ibu_kandung        String?        @db.VarChar(100)
  id_ms_status_kawin      Int            @default(0)
  status_kawin            MaritalStatus  @relation(fields: [id_ms_status_kawin], references: [id_ms_status_kawin])
  id_ms_golongan_darah    Int            @default(0)
  gologan_darah           BloodType      @relation(fields: [id_ms_golongan_darah], references: [id_ms_golongan_darah])
  id_ms_agama             Int
  agama                   Religion       @relation(fields: [id_ms_agama], references: [id_ms_agama])
  live                    Int            @default(1)
  rt_asal                 String         @db.VarChar(5)
  rw_asal                 String         @db.VarChar(5)
  id_ms_negara_asal       Int
  negara_asal             Country        @relation("negara_asal_pasien", fields: [id_ms_negara_asal], references: [id])
  id_ms_propinsi_asal     String         @db.VarChar(20)
  provinsi_asal           Province       @relation("provinsi_asal_pasien", fields: [id_ms_propinsi_asal], references: [id])
  id_ms_kota_asal         String         @db.VarChar(20)
  kota_asal               Regency        @relation("kota_asal_pasien", fields: [id_ms_kota_asal], references: [id])
  id_ms_kecamatan_asal    String         @db.VarChar(20)
  kecamatan_asal          District       @relation("kecamatan_asal_pasien", fields: [id_ms_kecamatan_asal], references: [id])
  id_ms_desa_asal         String         @db.VarChar(20)
  desa_asal               Village        @relation("desa_asal_pasien", fields: [id_ms_desa_asal], references: [id])
  alamat_asal             String         @db.Text
  alamatgab_asal          String         @db.Text
  nama_pekerjaan          String?        @db.VarChar(100)
  kode_pos_asal           String?        @db.VarChar(10)
  id_ms_pendidikan        Int
  pendidikan              EducationLevel @relation(fields: [id_ms_pendidikan], references: [id_ms_tingkat_pendidikan])
  created_at              DateTime       @default(now())
  created_by              Int            @default(0)
  modified_at             DateTime?      @db.DateTime(0)
  modified_by             Int?           @default(0)
  deleted_at              DateTime?      @db.DateTime(0)
  deleted_by              Int?           @default(0)
  restored_at             DateTime?      @db.DateTime(0)
  restored_by             Int?           @default(0)
  is_deleted              Boolean        @default(false)
  is_restored             Boolean        @default(false)

  @@index([nama_pasien])
  @@index([no_identitas])
  @@index([no_bpjs])
  @@index([no_hp])
  @@index([id_ms_provinsi_tinggal, id_ms_kota_tinggal])
  @@index([id_ms_kota_tinggal, id_ms_kecamatan_tinggal])
  @@index([tgl_lahir, jenis_kelamin])
  @@index([is_deleted, created_at])
  @@index([id_ms_negara_tinggal])
  @@index([id_ms_provinsi_tinggal])
  @@index([id_ms_kota_tinggal])
  @@index([id_ms_kecamatan_tinggal])
  @@index([id_ms_desa_tinggal])
  @@index([id_ms_negara_asal])
  @@index([id_ms_propinsi_asal])
  @@index([id_ms_kota_asal])
  @@index([id_ms_kecamatan_asal])
  @@index([id_ms_desa_asal])
  @@index([created_at])
  @@index([deleted_at])
  @@index([modified_at])
  @@map("db_pasien")
}

model Employee {
  id_pegawai              Int              @id @default(autoincrement())
  no_reg                  String           @db.VarChar(50)
  nip_pegawai             String?          @db.VarChar(50)
  nip_pns                 String?          @db.VarChar(50)
  gelar_depan             String?          @db.VarChar(100)
  gelar_belakang          String?          @db.VarChar(100)
  nama_pegawai            String           @db.VarChar(100)
  id_ms_negara_asal       Int
  negara_asal             Country?         @relation("CountryOfOrigin", fields: [id_ms_negara_asal], references: [id])
  id_ms_provinsi_asal     String?
  provinsi_asal           Province?        @relation("ProvinceOfOrigin", fields: [id_ms_provinsi_asal], references: [id])
  id_ms_kota_asal         String?
  kota_asal               Regency?         @relation("RegencyOfOrigin", fields: [id_ms_kota_asal], references: [id])
  id_ms_kecamatan_asal    String?
  kecamatan_asal          District?        @relation("DistrictOfOrigin", fields: [id_ms_kecamatan_asal], references: [id])
  id_ms_desa_asal         String?
  desa_asal               Village?         @relation("VillageOfOrigin", fields: [id_ms_desa_asal], references: [id])
  alamat_asal             String?          @db.Text
  kode_pos_asal           String?          @db.VarChar(10)
  rt_asal                 String?          @db.VarChar(5)
  rw_asal                 String?          @db.VarChar(5)
  id_ms_negara_tinggal    Int?
  negara_tinggal          Country?         @relation("CountryOfResidence", fields: [id_ms_negara_tinggal], references: [id])
  id_ms_provinsi_tinggal  String?
  provinsi_tinggal        Province?        @relation("ProvinceOfResidence", fields: [id_ms_provinsi_tinggal], references: [id])
  id_ms_kota_tinggal      String?
  kota_tinggal            Regency?         @relation("RegencyOfResidence", fields: [id_ms_kota_tinggal], references: [id])
  id_ms_kecamatan_tinggal String?
  kecamatan_tinggal       District?        @relation("DistrictOfResidence", fields: [id_ms_kecamatan_tinggal], references: [id])
  id_ms_desa_tinggal      String?
  desa_tinggal            Village?         @relation("VillageOfResidence", fields: [id_ms_desa_tinggal], references: [id])
  alamat_tinggal          String?          @db.Text
  kode_pos_tinggal        String?          @db.VarChar(10)
  rt_tinggal              String?          @db.VarChar(3)
  rw_tinggal              String?          @db.VarChar(3)
