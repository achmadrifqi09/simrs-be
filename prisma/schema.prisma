generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["relationJoins"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model AppClient {
  client_id     String    @id @default(uuid())
  client_secret String    @default(uuid())
  nama_client   String    @db.VarChar(100)
  created_at    DateTime  @default(now())
  created_by    Int       @default(0)
  modified_at   DateTime? @db.DateTime(0)
  modified_by   Int?      @default(0)
  deleted_at    DateTime? @db.DateTime(0)
  deleted_by    Int?      @default(0)
  restored_at   DateTime? @db.DateTime(0)
  restored_by   Int?      @default(0)
  is_deleted    Boolean   @default(false)
  is_restored   Boolean   @default(false)

  @@map("db_client")
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model Menu {
  id          Int              @id @default(autoincrement())
  label       String           @db.VarChar(100)
  order       Int              @unique
  icon        String?          @db.VarChar(100)
  pathname    String?          @db.VarChar(100)
  status      Boolean          @default(true)
  is_submenu  Boolean?         @default(false)
  submenu     Submenu[]
  izin_menu   MenuPermission[]
  created_at  DateTime         @default(now())
  created_by  Int              @default(0)
  modified_at DateTime?        @db.DateTime(0)
  modified_by Int?             @default(0)
  deleted_at  DateTime?        @db.DateTime(0)
  deleted_by  Int?             @default(0)
  restored_at DateTime?        @db.DateTime(0)
  restored_by Int?             @default(0)
  is_deleted  Boolean          @default(false)
  is_restored Boolean          @default(false)

  @@map("db_menu")
}

model Submenu {
  id           Int                 @id @default(autoincrement())
  label        String
  pathname     String
  id_menu      Int
  menu         Menu                @relation(fields: [id_menu], references: [id])
  izin_submenu SubmenuPermission[]
  created_at   DateTime            @default(now())
  created_by   Int                 @default(0)
  modified_at  DateTime?           @db.DateTime(0)
  modified_by  Int?                @default(0)
  deleted_at   DateTime?           @db.DateTime(0)
  deleted_by   Int?                @default(0)
  restored_at  DateTime?           @db.DateTime(0)
  restored_by  Int?                @default(0)
  is_deleted   Boolean             @default(false)
  is_restored  Boolean             @default(false)

  @@index([id_menu])
  @@map("db_submenu")
}

model MenuPermission {
  id          Int                 @id @default(autoincrement())
  nama        String
  izin_user   UserPermission[]
  id_menu     Int
  menu        Menu                @relation(fields: [id_menu], references: [id])
  submenu     SubmenuPermission[]
  is_create   Boolean
  is_update   Boolean
  is_delete   Boolean
  is_view     Boolean
  created_at  DateTime            @default(now())
  created_by  Int                 @default(0)
  modified_at DateTime?           @db.DateTime(0)
  modified_by Int?                @default(0)
  deleted_at  DateTime?           @db.DateTime(0)
  deleted_by  Int?                @default(0)
  restored_at DateTime?           @db.DateTime(0)
  restored_by Int?                @default(0)
  is_deleted  Boolean             @default(false)
  is_restored Boolean             @default(false)

  @@index([id_menu])
  @@map("db_izin_menu")
}

model SubmenuPermission {
  id           Int            @id @default(autoincrement())
  id_izin_menu Int
  izin_menu    MenuPermission @relation(fields: [id_izin_menu], references: [id])
  id_submenu   Int
  submenu      Submenu        @relation(fields: [id_submenu], references: [id])
  is_create    Boolean
  is_update    Boolean
  is_delete    Boolean
  is_view      Boolean
  created_at   DateTime       @default(now())
  created_by   Int            @default(0)
  modified_at  DateTime?      @db.DateTime(0)
  modified_by  Int?           @default(0)
  deleted_at   DateTime?      @db.DateTime(0)
  deleted_by   Int?           @default(0)
  restored_at  DateTime?      @db.DateTime(0)
  restored_by  Int?           @default(0)
  is_deleted   Boolean        @default(false)
  is_restored  Boolean        @default(false)

  @@index([id_izin_menu, id_submenu])
  @@map("db_izin_submenu")
}

model UserPermission {
  id           Int            @id @default(autoincrement())
  id_izin_menu Int
  izin_menu    MenuPermission @relation(fields: [id_izin_menu], references: [id])
  id_user      Int
  user         User           @relation(fields: [id_user], references: [id_user])
  created_at   DateTime       @default(now())
  created_by   Int            @default(0)
  modified_at  DateTime?      @db.DateTime(0)
  modified_by  Int?           @default(0)
  deleted_at   DateTime?      @db.DateTime(0)
  deleted_by   Int?           @default(0)
